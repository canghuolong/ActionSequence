{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "Runtime/**/*.cs",
            "UnityComponents/**/*.cs",
            "Extensions/**/*.cs"
          ],
          "src": ".."
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "*.md",
          "guides/**.md",
          "examples/**.md",
          "toc.yml"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appTitle": "ActionSequence Documentation",
      "_appName": "ActionSequence",
      "_appFooter": "Â© 2024 ActionSequence. All rights reserved.",
      "_enableSearch": true,
      "_enableNewTab": true
    }
  }
}
